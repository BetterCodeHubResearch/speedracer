{
  "name": "speedracer",
  "version": "0.2.0",
  "description": "Collect performance metrics for your library/application",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh/)",
  "license": "MIT",
  "repository": "ngryman/speedracer",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "speedracer": "./bin/speedracer.js"
  },
  "scripts": {
    "lint": "eslint bin lib test",
    "pretest": "npm run -s lint",
    "test": "cross-env NODE_ENV=test nyc ava",
    "start": "cross-env NODE_ENV=test ava --watch",
    "coverage": "nyc report --reporter=text-lcov | codecov",
    "see-coverage": "nyc report --reporter=html && open coverage/index.html"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "babel-generator": "^6.26.0",
    "babel-template": "^6.26.0",
    "babel-traverse": "^6.26.0",
    "babylon": "^6.18.0",
    "devtools-timeline-model": "^1.3.1",
    "hasha": "^3.0.0",
    "lodash": "^4.17.4",
    "pify": "^3.0.0",
    "puppeteer": "^0.11.0",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.8.0",
    "eslint-config-ngryman": "^1.7.1",
    "nyc": "^11.2.1",
    "p-using": "^1.2.0",
    "sinon": "^4.0.1",
    "stub-spawn-once": "^2.3.0"
  }
}
